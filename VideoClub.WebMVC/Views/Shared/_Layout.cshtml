<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - VIDEO CLUB VIRTUAL</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="index.html"><div class="sb-nav-link-icon"><i class="fas fa-film "></i> THE FILM ZONE</div></a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>

    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">Ubicaciones</div>

                        <a class="nav-link" href="@Url.Action("Index","Provincias")">
                            <div class="sb-nav-link-icon"><i class="fas fa-map "></i></div>
                            Provincias
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Localidad")">
                            <div class="sb-nav-link-icon"><i class="fas fa-map-marker-alt "></i></div>
                            Localidades
                        </a>

                        <div class="sb-sidenav-menu-heading">Peliculas</div>
                        <a class="nav-link" href="@Url.Action("Index","Soporte")">
                            <div class="sb-nav-link-icon"><i class="fas fa-compact-disc"></i></div>
                            Soportes
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Calificacion")">
                            <div class="sb-nav-link-icon"><i class="fas fa-star"></i></div>
                            Calificaciones
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Genero")">
                            <div class="sb-nav-link-icon"><i class="fas fa-theater-masks"></i></div>
                            Generos
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Estado")">
                            <div class="sb-nav-link-icon"><i class="fas fa-check-circle"></i></div>
                            Estados
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Pelicula")">
                            <div class="sb-nav-link-icon"><i class="fas fa-film"></i></div>
                            Peliculas
                        </a>

                        <div class="sb-sidenav-menu-heading">Personas</div>

                        <a class="nav-link" href="@Url.Action("Index","Socio")">
                            <div class="sb-nav-link-icon"><i class="fas fa-handshake"></i></div>
                            Socios
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Empleado")">
                            <div class="sb-nav-link-icon"><i class="fas fa-people-carry"></i></div>
                            Empleados
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Proveedor")">
                            <div class="sb-nav-link-icon"><i class="fab fa-product-hunt"></i></div>
                            Proveedores
                        </a>
                        <div class="sb-sidenav-menu-heading">Otro</div>
                        <a class="nav-link" href="@Url.Action("Index","TipoDeDocumento")">
                            <div class="sb-nav-link-icon"><i class="fas fa-address-card"></i></div>
                            Tipos de Documentos
                        </a>
                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Diseñado por :</div>
                    Matias Cuello
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    @RenderBody()
                </div>
            </main>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright &copy; Your Website 2022</div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body> 
</html>
